#define F_CPU 16000000UL
#include <avr/io.h>
#include <util/delay.h>
int main(void)
{
DDRA = 0xFF;
DDRB = 0xFF;
while(1){
PORTA= 0x3F ;
PORTB= 0x3F ;
_delay_ms(500);
PORTB= 0x06 ;
_delay_ms(500);
PORTB= 0x5B ;
_delay_ms(500);
PORTB= 0x4F ;
_delay_ms(500);
PORTB= 0x66 ;
_delay_ms(500);
PORTB= 0x6D ;
_delay_ms(500);
PORTB= 0x7D ;
_delay_ms(500);
PORTB= 0x07 ;
_delay_ms(500);
PORTB= 0x7F ;
_delay_ms(500);
PORTB= 0x6F ;
_delay_ms(500);
PORTA= 0x06 ;
PORTB= 0x3F ;
_delay_ms(500);
PORTB= 0x06 ;
_delay_ms(500);
PORTB= 0x5B ;
_delay_ms(500);
PORTB= 0x4F ;
_delay_ms(500);
PORTB= 0x66 ;
_delay_ms(500);
PORTB= 0x6D ;
_delay_ms(500);
PORTB= 0x7D ;
_delay_ms(500);
PORTB= 0x07 ;
_delay_ms(500);
PORTB= 0x7F ;
_delay_ms(500);
PORTB= 0x6F ;
_delay_ms(500);
}
}
